n.nav-home:hover {\nwidth: 45px;\n}\n\n.wrapper {\n   /*background-color: #eab;*/\n   display: block;\n   position: relative;\n   margin: 10vh auto;\n   width: 70%;\n   max-width: 1300px;\n\n   animation-name: wrapper;\n   animation: 1166ms ease 0s normal forwards 1 wrapper;\n}\n\n.picture {\n   display: flex;\n   flex-flow: row nowrap;\n   justify-content: center;\n   width: auto;\n   height: 600px;\n   margin: 0 10%;\n}\n\n.felicia {\n   flex-basis: 1 1 auto;\n   background-image: url(../img/felicia.jpg);\n   background-position: center;\n   background-size: cover;\n   background-repeat: no-repeat;\n   border: 2px solid black;\n   border-radius: 10px;\n   width: 100%;\n   height: 100%;\n}\n\n.text { \n   padding: 50px 10%;\n}\n\n.text h1 {\n   padding-bottom: 10px\n}\n\n.text p {\n   font: 15px Arial;\n   padding: 10px;\n   width: 100%;\n   word-spacing: auto;\n}\n\n/*@keyframes wrapper {\n   0% { opacity:0; transform: translateY(6%); }\n   8% { opacity:0; }\n   100% { opacity:1; transform: translateY(0%); }\n}*/\n\n@media (max-width: 800px) {\n   .wrapper {\n      width: 100%;\n   }\n}\n"},"lineCount":null}},"error":null,"hash":"b5cacfbfed57940ef816e280e2b2c60e","cacheData":{"env":{}}}